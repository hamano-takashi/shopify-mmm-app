# Shopify App (set by `shopify app dev`)
SHOPIFY_API_KEY=
SHOPIFY_API_SECRET=
SCOPES=read_orders,read_all_orders,read_reports

# Database (SQLite for dev, PostgreSQL for production)
# Dev (default):
DATABASE_URL="file:dev.sqlite"
# Production:
# DATABASE_URL="postgresql://postgres:password@localhost:5432/shopify_mmm"

# Redis (for BullMQ job queue)
REDIS_URL=redis://localhost:6379/0

# MMM Engine settings
MMM_CHAINS=4
MMM_TUNE=1000
MMM_DRAWS=500
MMM_MAX_CONCURRENT=2
